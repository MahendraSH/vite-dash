{
  "form-heading": "Feedback Form",
  "form-description": "We value your feedback! Please take a moment to share your thoughts with us.",
  "form-items": [
    {
      "label": "Name",
      "type": "input-text",
      "name": "name"
    },
    {
      "label": "Email",
      "type": "input-text",
      "name": "email"
    },
    {
      "label": "Feedback Category",
      "type": "select",
      "options": ["General", "Website", "Product", "Customer Service"],
      "name": "feedbackCategory"
    },
    {
      "label": "Feedback Details",
      "type": "text-area",
      "name": "feedbackDetails"
    },
    {
      "label": "Rating",
      "type": "radio",
      "options": ["1", "2", "3", "4", "5"],
      "name": "rating"
    }
  ],
  "form-btns": [
    {
      "label": "Submit Feedback",
      "type": "submit"
    },
    {
      "label": "Clear",
      "type": "reset"
    }
  ]
}
